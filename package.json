{
  "name": "react-apollo-graphql-prisma-starter",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "jwt-decode": "2.2.0",
    "prettier-eslint-cli": "4.7.1"
  },
  "dependencies": {
    "apollo-boost": "0.1.10",
    "apollo-client": "2.3.5",
    "apollo-client-preset": "1.0.8",
    "apollo-link": "1.2.2",
    "apollo-link-ws": "1.0.8",
    "apollo-utilities": "1.0.16",
    "autoprefixer": "8.6.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-apollo": "2.1.8",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.4.1",
    "react-modal": "3.4.5",
    "react-router-dom": "4.3.1",
    "resolve": "1.8.1",
    "style-loader": "0.21.0",
    "subscriptions-transport-ws": "0.9.12",
    "sw-precache-webpack-plugin": "0.11.5",
    "tachyons": "4.10.0",
    "url-loader": "1.0.1",
    "webpack": "4.14.0",
    "webpack-dev-server": "3.1.4",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "precommit": "yarn run lint && yarn run flow",
    "flow": "flow",
    "lint": "eslint .",
    "setup": "docker-compose -f server/database/docker-compose.yml up -d",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prettier": "prettier-eslint --write \"src/**/*.+(js|jsx)\"",
    "prettier-dry-run": "prettier-eslint \"src/**/*.+(js|jsx)\" -l debug"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
